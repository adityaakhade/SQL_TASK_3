--SELECTING * 

SELECT * FROM CUSTOMERS;
SELECT * FROM CATEGORIES;

--SELECTING SPECIFIC COLUMNS
SELECT NAME 
FROM CUSTOMERS;

SELECT PRICE FROM ORDER_ITEMS;

SELECT EMAIL FROM CUSTOMERS;

SELECT PHONE FROM CUSTOMERS;

--USING WHERE CLAUSE 

SELECT phone ,NAME , EMAIL FROM CUSTOMERS 
WHERE Phone=7719840410

SELECT * 
 FROM CUSTOMERS 
 where name ='RUSHIKESH BHONDAVE ';
 

SELECT * 
 FROM CUSTOMERS 
 WHERE PHONE = 9067541415  AND NAME ='RUSHIKESH BHONDAVE ';
 
SELECT * 
 FROM CUSTOMERS 
 WHERE PHONE = 7719840410 OR NAME ='RUSHIKESH BHONDAVE ';
 
--USING LIKE OPERATOR
 SELECT * 
 FROM CUSTOMERS 
 WHERE NAME LIKE 'A%';
 
SELECT * 
 FROM CUSTOMERS 
 WHERE NAME LIKE '%A%';
 
--BETWEEN OPERATOR
SELECT * FROM ORDER_ITEM
WHERE PRICE BETWEEN 10 AND 100;
 

 



 





